{
	"hosts": [
		{
			"vm": "<pica01|pica02|monitoring>",
			"host": "<vm>.picasoft.net",
			"exclude": [
				"[container_name]"
			],
			"tls_config":
			{
				"ca_cert": "/DATA/ca.pem",
