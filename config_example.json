{
	"organization": "Picasoft",
	"merge": true
	"hosts": [
		{
			"vm": "<pica01|pica02|monitoring>",
			"host_url": "<vm>.picasoft.net",
			"port": 2376
			"exclude": [
				"[container_name]"
			],
			"tls_config":
			{
				"ca_cert": "/DATA/ca.pem",
				"cert": "/DATA/cert.pem",
				"key": "/DATA/key.pem"
			}
		},
		{
			"vm": "<pica01|pica02|monitoring>",
			"host": "localhost"
		}
	],
	"color_scheme":
	{
		"traefik": "#edb591",
		"port": "#86c49b",
		"link": "#75e9cd",
		"image": "#e1efe6",
		"container": "#ffffff",
		"network": "#ffffff",
		"vm": "#e1efe6",
		"dark_text": "#32384f",
		"bright_text": "#ffffff"
	}
}
