{
	"organization": "Picasoft",
	"merge": true
	"hosts": [
		{
			"vm": "<vm1>",
			"host_url": "<vm1>.tld",
			"port": 2376,
			"exclude": [
				"[container_name]"
			],
			"tls_config":
			{
				"ca_cert": "/CONFIG/ca.pem",
				"cert": "/CONFIG/cert.pem",
				"key": "/CONFIG/key.pem"
			}
		},
		{
			"vm": "<vm2>",
			"host": "localhost"
		}
	],
	"color_scheme":
	{
		"traefik": "#edb591",
		"port": "#86c49b",
		"link": "#75e9cd",
		"image": "#e1efe6",
		"container": "#ffffff",
		"network": "#ffffff",
		"vm": "#e1efe6",
		"dark_text": "#32384f",
		"bright_text": "#ffffff"
	},
	"actions": [
		{
			"type": "webdav",
			"hostname": "https://example.com/nextcloud/remote.php/dav/files/<login>",
			"login": "login",
			"password": "password",
			"remote_path": "graph_output"
		}
	]
}
